{
  "env": {
    "test": {
      "plugins": [
        "istanbul"
      ]
    }
  },
  "presets": ["es2015"],
  "plugins": [
    "add-module-exports",
    ["transform-es2015-modules-umd", {
      "globals": {
        "lodash": "_",
        "jquery": "$"
      },
      "exactGlobals": true
    }]
  ]
}
